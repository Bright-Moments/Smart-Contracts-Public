pragma solidity 0.8.19;
import { Ownable } from "@openzeppelin/contracts/access/Ownable.sol";
contract MarketplaceHelper is Ownable
{
    address public _MARKETPLACE = 0x2923A537546443AdcF47c7351b51Da6E423d3e58;
    address public _NFT_ADDRESS = 0x7B807B46Ee5B65dD20848EE6c805C4A86d06d3Ce;
    uint public _MINTER_PROJECT_ID = 22;

    /**
     * @dev Marketplace Handler To Mint MintPacks
     */
    function _MintToFactory(uint BatchPurchaseAmount, address Recipient, uint DesiredAmount) external onlyMarketplace
    {
        IERC721 _NFT = IERC721(_NFT_ADDRESS);
        uint _ETHValue = 0.999 ether / BatchPurchaseAmount;
        uint _MinterProjectID = 22;
        for(uint x; x < DesiredAmount; x++) // Iterates Over The Desired Units Of Purchase
        {
            for(uint y; y < BatchPurchaseAmount; y++) // Mints Configured Amount Of NFTs Per Unit Purchased
            { 
                _NFT.purchaseTo{value: _ETHValue}(Recipient, _MinterProjectID); 
            }
        }
    }

    /**
     * @dev Change Marketplace Address
     * @param NewAddress New Marketplace Address
     */
    function __ChangeMarketplaceAddress(address NewAddress) external onlyOwner { _MARKETPLACE = NewAddress; }

    /**
     * @dev Change MintPack Amount
     * @param NewAmount New MintPack Amount
     */
    function __ChangeMintPackAmount(uint NewAmount) external onlyOwner { _MINTER_PROJECT_ID = NewAmount; }

    /**
     * @dev Change NFT Address
     * @param NewAddress New NFT Address
     */
    function __ChangeNFTAddress(address NewAddress) external onlyOwner { _NFT_ADDRESS = NewAddress; }

    /**
     * @dev Access Modifier To Restrict Function To Marketplace
     */
    modifier onlyMarketplace
    {
        require(msg.sender == _MARKETPLACE, "MarketplaceHelper: Only Marketplace");
        _;
    }
}

interface IMintPass { function _MintToFactory(address Recipient, uint DesiredAmount) external; }
interface IERC721 { function purchaseTo(address Recipient, uint ProjectID) external payable; } // MintPack Purchase Function
/**
 * @dev: @brougkr
 */
pragma solidity 0.8.19;
contract QQLMinter
{
    address public _QQL_ADDRESS = 0x845dD2a7eE2a92A0518AB2135365Ed63fdbA0C88;
    address public _QQL_MP = 0xc73B17179Bf0C59cD5860Bb25247D1D1092c1088;
    mapping(address=>bool) public Minter;
    event LiveMintCompleteArtist(address Recipient, bytes32 Seed, uint TicketID, uint MintedWorkTokenID);

    constructor()
    {
        Minter[0xe06F5FAE754e81Bc050215fF89B03d9e9FF20700] = true; // `operator.brightmoments.eth`
        Minter[0x1A0a3E3AE390a0710f8A6d00587082273eA8F6C9] = true; // BRT Minter #1
        Minter[0x4d8013b0c264034CBf22De9DF33e22f58D52F207] = true; // BRT Minter #2
        Minter[0x4D9A8CF2fE52b8D49C7F7EAA87b2886c2bCB4160] = true; // BRT Minter #3
        Minter[0x124fd966A0D83aA020D3C54AE2c9f4800b46F460] = true; // BRT Minter #4
        Minter[0x100469feA90Ac1Fe1073E1B2b5c020A8413635c4] = true; // BRT Minter #5
        Minter[0x756De4236373fd17652b377315954ca327412bBA] = true; // BRT Minter #6
        Minter[0xc5Dfba6ef7803665C1BDE478B51Bd7eB257A2Cb9] = true; // BRT Minter #7
        Minter[0xFBF32b29Bcf8fEe32d43a4Bfd3e7249daec457C0] = true; // BRT Minter #8
        Minter[0xF2A15A83DEE7f03C70936449037d65a1C100FF27] = true; // BRT Minter #9
        Minter[0x1D2BAB965a4bB72f177Cd641C7BacF3d8257230D] = true; // BRT Minter #10
        Minter[0x2e51E8b950D72BDf003b58E357C2BA28FB77c7fB] = true; // BRT Minter #11
        Minter[0x8a7186dECb91Da854090be8226222eA42c5eeCb6] = true; // BRT Minter #12
        Minter[0x5778B0B140Fa7a62B96c193cC8621e6E96c088A5] = true; // BRT Minter #13
    }
    
    /**
     * @dev Live Mints QQL note: Requires Owner Of QQL Mint Pass To setApprovalForAll() To This Contract
     */
    function _LiveMintQQL(uint[] calldata TicketIDs, bytes32[] calldata Seeds) external onlyMinter
    {
        for(uint x; x < TicketIDs.length; x++)
        {
            address Recipient = IQQL(_QQL_MP).ownerOf(TicketIDs[x]);
            uint MintedWorkTokenID = IQQL(_QQL_ADDRESS).mintTo(TicketIDs[x], Seeds[x], Recipient);
            emit LiveMintCompleteArtist(Recipient, Seeds[x], TicketIDs[x], MintedWorkTokenID);
        }
    }

    /**
     * @dev Withdraws Any NFT Mistakenly Sent To This Contract
     */
    function _Withdraw(address NFT, uint TokenID) external onlyMinter { IERC721(NFT).transferFrom(address(this), msg.sender, TokenID); }

    /**
     * @dev onlyMinter Access Modifier
     */
    modifier onlyMinter
    {
        require(Minter[msg.sender], "onlyMinter: Caller Is Not Authorized Minter");
        _;
    }
}

interface IQQL
{
    function mintTo(uint mintPassId, bytes32 seed, address recipient) external returns (uint);
    function ownerOf(uint tokenId) external view returns (address);
}

interface IERC721
{
    function transferFrom(address From, address To, uint TokenID) external;
}
pragma solidity 0.8.19;
import { Ownable } from "@openzeppelin/contracts/access/Ownable.sol";
import { ERC721A } from "./ERC721A.sol";
import { LiveMintEnabled } from "./LiveMintEnabled.sol";
contract Harm is Ownable, ERC721A, LiveMintEnabled
{
    uint public _TOTAL_SUPPLY = 100;
    address public _LIVE_MINT;
    address public _METADATA_CONTROLLER = 0x0721D84598F5cb5bC2EC381489D704064c7011Ed;

    constructor() ERC721A("Harm Test", "Harm") { }

    /**
     * @dev Mints A Token To The Recipient
     */
    function purchaseTo(address Recipient, uint ProjectID) override payable external virtual onlyLiveMint returns (uint tokenID) 
    {
        uint _Total_Minted = totalSupply();
        require(_Total_Minted + 1 <= _TOTAL_SUPPLY, "No More Tokens To Mint");
        uint TokenIDToBe = _Total_Minted;
        _mint(Recipient, 1);
        return TokenIDToBe;
    }

    /**
     * @dev Instantiates New LiveMint Address
     */
    function _ChangeLiveMintAddress(address NewAddress) external override onlyOwner { _LIVE_MINT = NewAddress; }

    /**
     * @dev Instantiates New Metadata Controller Address
     */
    function _ChangeMetadataController(address NewAddress) external onlyOwner { _METADATA_CONTROLLER = NewAddress; }

    /**
     * @dev Executes Arbitrary Transaction(s)
     */
    function ___Execute(address[] memory Targets, uint[] memory Values, bytes[] memory Datas) external onlyOwner
    {
        for (uint x; x < Targets.length; x++) 
        {
            (bool success,) = Targets[x].call{value:(Values[x])}(Datas[x]);
            require(success, "i have failed u anakin");
        }
    }

    /**
     * @dev Withdraws All Ether From The Contract
     */
    function ___WithdrawEther() external onlyOwner { payable(msg.sender).transfer(address(this).balance); }

    /**
     * @dev Withdraws Ether From Contract To Address With An Amount
     */
    function ___WithdrawEtherToAddress(address payable Recipient, uint Amount) external onlyOwner
    {
        require(Amount > 0 && Amount <= address(this).balance, "Invalid Amount");
        (bool Success, ) = Recipient.call{value: Amount}("");
        require(Success, "Unable to Withdraw, Recipient May Have Reverted");
    }

    /**
     * @dev Overrides TokenURI Method To Use An External Metadata Controller
     */
    function tokenURI(uint tokenID) public view virtual override returns (string memory)
    {
        return IMetadataController(_METADATA_CONTROLLER).tokenURI(tokenID);
    }
}

interface IMetadataController { function tokenURI(uint tokenID) external view returns (string memory); }
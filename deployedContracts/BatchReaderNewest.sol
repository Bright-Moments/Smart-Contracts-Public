pragma solidity 0.8.19;
contract BatchReader 
{
    struct CityInfo
    {
        string Name;
        address NFT;
        uint StartingIndex;
    }
    mapping(uint=>CityInfo) public CityInformation;
    
    constructor()
    {
        CityInformation[0] = CityInfo('CryptoGalacticans', 0xbDdE08BD57e5C9fD563eE7aC61618CB2ECdc0ce0, 0);
        CityInformation[1] = CityInfo('CryptoVenetians', 0xa7d8d9ef8D8Ce8992Df33D8b8CF4Aebabd5bD270, 95000000);
        CityInformation[2] = CityInfo('CryptoNewYorkers', 0xa7d8d9ef8D8Ce8992Df33D8b8CF4Aebabd5bD270, 189000000);
        CityInformation[3] = CityInfo('CryptoBerliners', 0xbDdE08BD57e5C9fD563eE7aC61618CB2ECdc0ce0, 3000000);
        CityInformation[4] = CityInfo('CryptoLondoners', 0xbDdE08BD57e5C9fD563eE7aC61618CB2ECdc0ce0, 4000000);
        CityInformation[5] = CityInfo('CryptoMexas', 0xbDdE08BD57e5C9fD563eE7aC61618CB2ECdc0ce0, 5000000);
        CityInformation[6] = CityInfo('CryptoTokyites', 0xbDdE08BD57e5C9fD563eE7aC61618CB2ECdc0ce0, 6000000);
        CityInformation[7] = CityInfo('City #8', 0xbDdE08BD57e5C9fD563eE7aC61618CB2ECdc0ce0, 7000000);
        CityInformation[8] = CityInfo('City #9', 0xbDdE08BD57e5C9fD563eE7aC61618CB2ECdc0ce0, 8000000);
        CityInformation[9] = CityInfo('City #10', 0xbDdE08BD57e5C9fD563eE7aC61618CB2ECdc0ce0, 9000000);
    }

    /**
     * @dev Returns An Array Of A Specific CryptoCitizen City Owners
     */
    function ReadCitizenCityOwners(uint CityIndex) external view returns (address[] memory)
    {
        address NFTAddress = CityInformation[CityIndex].NFT;
        IERC721 _NFT = IERC721(NFTAddress);
        address[] memory Owners = new address[](1000);
        uint Start = CityInformation[CityIndex].StartingIndex;
        uint End = Start + 1000;
        for(Start; Start < End; Start++)
        {
            try _NFT.ownerOf(Start) returns (address Owner) { Owners[Start] = Owner; }
            catch { continue; }
        }
        return Owners;
    }

    /**
     * @dev Returns An Address Array Of Owners Of A Specific NFT
     */
    function ReadNFTOwnedTokenIDs(address NFT, uint StartingIndex, uint Endingindex) external view returns (address[] memory Owners)
    {
        IERC721 _NFT = IERC721(NFT);
        Owners = new address[](Endingindex - StartingIndex);
        for(StartingIndex; StartingIndex < Endingindex; StartingIndex++)
        {
            try _NFT.ownerOf(StartingIndex) returns (address Owner) { Owners[StartingIndex] = Owner; }
            catch { continue; }
        }
        return Owners;
    }

    /**
     * @dev Returns A Wallet's Owned Moments NFTs
     */
    function readNFTsOwnedTokenIDs(
        address Wallet,
        address[] calldata NFTs,
        uint[] calldata ProjectIDs, // If ArtBlocks
        uint Range
    ) public view returns(uint[][] memory) {
        uint[][] memory OwnedIDs = new uint[][](ProjectIDs.length);
        for(uint x; x < ProjectIDs.length; x++)
        {
            IERC721 NFT = IERC721(NFTs[x]);
            uint[] memory temp = new uint[](Range);
            uint _TokenID = ProjectIDs[x] * 1000000;
            uint _Range = _TokenID + Range;
            uint NumOwnedTokens;
            for(_TokenID; _TokenID < _Range; _TokenID++)
            {
                try NFT.ownerOf(_TokenID) returns (address _Owner)
                {
                    if(_Owner == Wallet)
                    {
                        temp[NumOwnedTokens] = _TokenID;
                        NumOwnedTokens++;
                    }
                } catch { }
            }
            uint[] memory FormattedOwnedIDs = new uint[](NumOwnedTokens);
            for(uint z; z < NumOwnedTokens; z++) { FormattedOwnedIDs[z] = temp[z]; }
            OwnedIDs[x] = FormattedOwnedIDs;
        }
        return OwnedIDs;
    }
}

interface IERC721 { function ownerOf(uint256 tokenId) external view returns (address owner); }
pragma solidity 0.8.19;
contract LiveMintHelper
{   
    // address private constant _LIVE_MINT = 0x7F8ED45c0790A5cFaAbeFe995f5694214C37F1a6; //mainnet
    // address private constant _VENSTER = 0x49c804f27153ec24C332A5aed3a8823a7a44c900; // mainnet

    address private constant _LIVE_MINT = 0xe7f1725E7734CE288F8367e1Bb143E90bb3F0512; // hardhat
    address private constant _VENSTER = 0xDc64a140Aa3E981100a9becA4E685f962f0cF6C9; // hardhat
    IVenster private constant Venster = IVenster(_VENSTER);

    /**
     * @dev Purchase a Venster Token To A Specific Address
     */
    function purchaseTo(address Recipient, uint ProjectID) external payable returns (uint TokenID)
    {
        require(msg.sender == _LIVE_MINT, "Helper: Caller Is Not LiveMint");
        require(ProjectID == 0, "Helper: Wrong MintPass ID Provided For ArtistID");
        return Venster.purchaseTo(Recipient, ProjectID);
    }
}

interface IVenster { function purchaseTo(address Recipient, uint ProjectID) external payable returns (uint TokenID); }
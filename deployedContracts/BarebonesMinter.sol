pragma solidity 0.8.19;
import { MissionControl } from "./MissionControl.sol";
import { ReentrancyGuard } from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
contract BarebonesMinter is MissionControl, ReentrancyGuard
{
    uint public _PRICE = 0.1 ether;
    address public _NFT = 0xb642F85D2B23b16aC310cC0D462716d80608B25C;
    address public _MULTISIG = 0x0BC56e3c1397e4570069e89C07936A5c6020e3BE;

    /**
     * @dev Emitted When NFTs Are Purchased
     */
    event Purchased(address Purchaser, uint Amount);

    /**
     * @dev Purchase NFTs
     * @param Amount The Amount of NFTs to Purchase
     */
    function PurchaseBarebones(uint Amount) external payable 
    {
        require(tx.origin == msg.sender, "EOA Only");
        require(msg.value == _PRICE * Amount, "Invalid ETH Sent");
        IERC721(_NFT)._MintToFactory(msg.sender, Amount);
        emit Purchased(msg.sender, Amount);
    }

    /**
     * @dev Changes The Address of The NFT
     * @param _NewAddress The New Address of The NFT
     */
    function _ChangeNFTAddress(address _NewAddress) external onlyOperator { _NFT = _NewAddress; }

    /**
     * @dev Changes The Price
     * @param _NewPrice The New Price
     */
    function _ChangePrice(uint _NewPrice) external onlyOperator { _PRICE = _NewPrice; }

    /**
     * @dev Withdraw ETH From The Contract
     */
    function _WithdrawETHToMultisig() external onlyOperator
    {
        (bool success, ) = _MULTISIG.call{value: address(this).balance}("");
        require(success, "Transfer failed.");
    }

    /**
     * @dev Withdraws ETH From The Contract
     */
    function ___WithdrawETHToOwner() external onlyOwner { payable(msg.sender).transfer(address(this).balance); }

    /**
     * @dev Withdraws ETH From The Contract
     */
    function ___WithdrawETHToOwnerCall() external onlyOwner 
    { 
        (bool success, ) = msg.sender.call{value: address(this).balance}(""); 
        require(success, "Transfer failed."); 
    }
}

interface IERC721 { function _MintToFactory(address _to, uint256 _tokenId) external; }
/**
 * @dev: @brougkr
 * @title: Mint
 * @notice: This Contract Is The ERC721 Base Contract For All Mint Contracts
 */
pragma solidity 0.8.19;
import { MissionControl } from "./MissionControl.sol";
import { ERC721A } from "./ERC721A.sol";
contract Mint is MissionControl, ERC721A
{
    struct MintInfo
    {
        string _Name;             // [0] -> _Name
        string _Symbol;           // [1] -> _Symbol
        string _MetadataURI;      // [2] -> _MetadataURI
        uint _MaxSupply;          // [3] -> _MaxSupply
        uint _MintsPerPack;       // [4] -> _MintsPerPack
        uint _ProjectID;          // [5] -> _ProjectID note: For Cases Where Minting Custom ERC721 Or ArtBlocks
        uint _ReserveAmount;      // [6] -> _Reserve
        address _Marketplace;     // [7] -> _Marketplace
        address _LiveMint;        // [8] -> _LiveMint
    }

    address private constant _Multisig = 0xB96E81f80b3AEEf65CB6d0E280b15FD5DBE71937; // Multisig Address
    uint[] public MintPackIndexes;

    /**
     * @dev Parameters For The MintPass Contract
     */
    MintInfo public _MintInfo;

    /**
     * @dev Mint Pass Factory Constructor
     * @param _Info The MintInfo
     */
    constructor ( MintInfo memory _Info ) ERC721A ( _Info._Name, _Info._Symbol ) 
    { 
        _MintInfo = _Info; 
        if(_Info._ReserveAmount > 0) { _mint(_Multisig, _Info._ReserveAmount); }
    }

    /**
     * @dev Direct Mint Function
     * @param Recipient The Address Of The Recipient
     */
    function _MintToFactory(address Recipient, uint Amount) external onlyMarketplace
    {
        require(totalSupply() + Amount <= _MintInfo._MaxSupply, "MintPass: Amount Exceeds Max Supply");
        _mint(Recipient, Amount);
    }

    /**
     * @dev Direct Mint To Factory Pack
     * @param Recipient The Address Of The Recipient
     */
    function _MintToFactoryPack(address Recipient, uint Amount) external onlyMarketplace
    {
        uint StartingTokenID = totalSupply();
        uint NumArtists = _MintInfo._MintsPerPack;
        uint NumToMint = NumArtists * Amount;
        require(StartingTokenID + NumToMint <= _MintInfo._MaxSupply, "MintPass: Desired Purchase Amount Exceeds Max Supply");
        _mint(Recipient, NumToMint);
        for(uint x; x < Amount; x++) { MintPackIndexes.push(StartingTokenID + (NumArtists * x)); }
    }

    /**
     * @dev LiveMint Redeems Mint Pass If Not Already Burned & Sends Minted Work To Owner's Wallet
     * @param TokenID The TokenID To Redeem
     * @return _Recipient The Address Of The Recipient
     * @return _ArtistID The ArtistID
     */
    function _LiveMintBurn(uint TokenID) external onlyLiveMint returns (address _Recipient, uint _ArtistID)
    {
        address Recipient = IERC721(address(this)).ownerOf(TokenID);
        require(Recipient != address(0), "MintPass: Invalid Recipient");
        _burn(TokenID, false);
        return (Recipient, _MintInfo._ProjectID);
    }

    /**
     * @dev Updates The BaseURI For A Project
     * @param NewURI The New BaseURI
     */
    function __NewBaseURI(string memory NewURI) external onlyAdmin { _MintInfo._MetadataURI = NewURI; }

    /**
     * @dev Updates The LiveMint Address
     * @param _NewAddress The New LiveMint Address
     */
    function __OverrideLiveMintAddress(address _NewAddress) external onlyAdmin { _MintInfo._LiveMint = _NewAddress; }

    /**
     * @dev Updates The Marketplace Address
     * @param _NewAddress The New Marketplace Address
     */
    function __OverrideMarketplaceAddress(address _NewAddress) external onlyAdmin { _MintInfo._Marketplace = _NewAddress; }

    /**
     * @dev Overrides The Max Supply For A MintPassProjectID
     * @param NewMaxSupply The New Max Supply
     */
    function __OverrideMaxSupply(uint NewMaxSupply) external onlyAdmin { _MintInfo._MaxSupply = NewMaxSupply; }

    /**
     * @dev Owner Burn Function
     * @param TokenIDs The TokenIDs To Burn
     */
    function __OverrideBurn(uint[] calldata TokenIDs) external onlyAdmin { for(uint x; x < TokenIDs.length; x++) { _burn(TokenIDs[x], false); } }

    /**
     * @dev Returns Base URI Of Desired TokenID
     * @return _BaseURI The BaseURI
     */
    function _baseURI() internal override view returns (string memory) { return _MintInfo._MetadataURI; }

    /**
     * @dev Access Modifier For Marketplace Smart Contracts
     * note: This Is A Custom Access Modifier That Is Used To Restrict Access To Only Authorized Contracts
     */
    modifier onlyMarketplace
    {
        require(_MintInfo._Marketplace == msg.sender, "MintPass | onlyMarketplace | Caller Is Not Marketplace");
        _;
    }

    /**
     * @dev Access Modifier For The LiveMint Smart Contracts
     */
    modifier onlyLiveMint
    {
        require(_MintInfo._LiveMint == msg.sender, "MintPass | onlyLiveMint | Caller Is Not LiveMint");
        _;
    }
}

/**
 * @dev @brougkr
 * @title MintPassFactory
 * @notice This Contract Is Used To Deploy New Mint Pass Contracts
 */
contract MintPassFactory is MissionControl
{   
    /**
     * @dev Emitted When A Mint Pass Is Deployed
     */
    event MintPassDeployed ( address MintPassAddress );

    /**
     * @dev Creates A New MintPass Contract
     * @param _MintInfo The MintInfo
     * @return MintPassAddress The Address Of The New ERC721 Contract 
     */
    function _InitMintPass ( Mint.MintInfo memory _MintInfo ) external onlyOperator returns ( address MintPassAddress )
    {
        MintPassAddress = address ( new Mint ( _MintInfo ) );
        emit MintPassDeployed ( MintPassAddress );
        return MintPassAddress;
    }
}

interface IERC721 { function ownerOf(uint TokenID) external returns (address); }
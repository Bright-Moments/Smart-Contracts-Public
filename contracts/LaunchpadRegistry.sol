/** 
 * @dev @brougkr
 * @title LaunchpadRegistry
 * @notice The Launchpad Registry Contains Information About All Projects That Have Been Created Through The Launchpad Contract
 */
pragma solidity 0.8.19;
import { MissionControl } from "./MissionControl.sol";
contract LaunchpadRegistry is MissionControl
{   
    struct Project
    {
        string _Name;
        bool _Active;
        uint _LiveMintArtistID;
        uint _MaxSupply;
        uint _MintsPerPack;
        uint _SaleIndex;
        address _MintPassAddress;
        address _MarketplaceAddress;
        address _MintedWorkAddress;
    }

    /*-----------------------------------------------------------------------------*/
    /*------------------ * STATE VARIABLES & MAPPINGS & EVENTS * ------------------*/
    /*-----------------------------------------------------------------------------*/
    
    uint public UniqueProjectsInvoked;
    mapping(uint=>Project) public Projects;
    mapping(address => uint) public ProjectMapping;
    mapping(address=>bytes32) public Role;
    event OwnershipTransferred(address OldOwner, address NewOwner);

    /*---------------------------------------------------------*/
    /*------------------ * ADMIN FUNCTIONS * ------------------*/
    /*---------------------------------------------------------*/

    /**
     * @dev Initializes A New Project Into The Launchpad Registry
     */
    function _NewProject(Project memory _Project) external onlyOperator returns (uint LaunchpadProjectID)
    {
        LaunchpadProjectID = UniqueProjectsInvoked;
        Projects[LaunchpadProjectID] = _Project;
        UniqueProjectsInvoked = LaunchpadProjectID + 1;
        ProjectMapping[_Project._MintPassAddress] = LaunchpadProjectID;
        return LaunchpadProjectID;
    }

    /*--------------------------------------------------------*/
    /*------------------ * VIEW FUNCTIONS * ------------------*/
    /*--------------------------------------------------------*/

    /**
     * @dev Returns The Dutch Auction Sale Index For A Project
     * @param LaunchpadProjectID The Launchpad ProjectID
     * @return SaleIndex The Sale Index
     */
    function ViewSaleIndex(uint LaunchpadProjectID) public view returns ( uint SaleIndex )
    {
        return Projects[LaunchpadProjectID]._SaleIndex;
    }

    /**
     * @dev Returns The SaleID & Contract Address For A Project
     * @param LaunchpadProjectID The Launchpad ProjectID
     * @return SaleIndex          The Sale Index
     * @return SaleContractAddress The Sale Contract Address
     */
    function ViewSaleInfo(uint LaunchpadProjectID) public view returns ( uint SaleIndex, address SaleContractAddress)
    {
        return (Projects[LaunchpadProjectID]._SaleIndex, Projects[LaunchpadProjectID]._MarketplaceAddress);
    }

    /**
     * @dev Views A Project
     * @param LaunchpadProjectID | The Launchpad ProjectID
     * @return LaunchpadProject  | Launchpad Project
     */
    function ViewProject(uint LaunchpadProjectID) public view returns(Project memory LaunchpadProject) { return Projects[LaunchpadProjectID]; }

    /**
     * @dev Returns An Array Of Projects
     * @param Indexes   | The Indexes Of The Projects
     * @return Projects | Launchpad Projects
     */
    function ViewProjects(uint[] calldata Indexes) public view returns(Project[] memory) 
    { 
        Project[] memory _Projects = new Project[](Indexes.length);
        for(uint x; x < Indexes.length; x++) { _Projects[x] = Projects[Indexes[x]]; }
        return _Projects;
    }
}
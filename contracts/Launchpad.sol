interface IMerkle
{
    struct Claim
    {
        string _Name;            // [0] -> _Name
        bytes32 _Root;           // [1] -> _Root
        address _NFT;            // [2] -> _NFT
        address _Operator;       // [3] -> _Operator
        address _BurnNFT;        // [4] -> _BurnNFT
        uint _ClaimCost;         // [5] -> _ClaimCost
        uint _ProjectID;         // [6] -> _ProjectID
        uint _ClaimableAmount;   // [7] -> _ClaimableAmount
        uint _AmountClaimed;     // [8] -> _AmountClaimed
        uint _StartingTimestamp; // [9] -> _StartingTimestamp
        uint _SecondsInterval;   // [10] -> _SecondsInterval
    }

    /**
     * @dev Starts A New Merkle Claim
     */
    function _NewClaim(Claim memory _NewClaim) external returns (uint ClaimIndex);

    /**
     * @dev Overrides A Merkle Claim
     */
    function _OverrideMerkleClaim(uint ClaimIndex, Claim memory _NewClaim) external;

    /**
     * @dev Changes The Current Merkle Root At The Provided Claim Index 
     */
    function _ChangeRoot(uint ClaimIndex, bytes32 Root) external;

    /**
     * @dev Returns The Claim Information For The Provided Claim Index
     */
    function ViewClaim(uint ClaimIndex) external view returns (Claim memory);

    /**
     * @dev Returns Claim Information For The Provided Claim Indexes
     */
    function ViewClaims(uint[] calldata ClaimIndexes) external view returns (Claim[] memory);
}


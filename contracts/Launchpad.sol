    function __UpgradeMerkleClaimer(address NewAddress) external onlyOwner { ICustom(Params._MerkleClaimer).____LaunchpadOverrideAddress(NewAddress); }

    /**
     * @dev Initiates Upgrade Of The Launchpad With All Current Active Contracts
     * @param NewLaunchpadAddress The New Launchpad Address
     */
    function ___UpgradeAll(address NewLaunchpadAddress) external onlyOwner
    {
        ICustom(Params._DutchMarketplace).____LaunchpadOverrideAddress(NewLaunchpadAddress);
        ICustom(Params._DynamicMarketplace).____LaunchpadOverrideAddress(NewLaunchpadAddress);
        ICustom(Params._EnglishMarketplace).____LaunchpadOverrideAddress(NewLaunchpadAddress);
        ICustom(Params._LiveMint).____LaunchpadOverrideAddress(NewLaunchpadAddress);
        ICustom(Params._FactoryMintedWorks).____LaunchpadOverrideAddress(NewLaunchpadAddress);
        ICustom(Params._FactoryMintPass).____LaunchpadOverrideAddress(NewLaunchpadAddress);
        ICustom(Params._LaunchpadRegistry).____LaunchpadOverrideAddress(NewLaunchpadAddress);
        ICustom(Params._MerkleClaimer).____LaunchpadOverrideAddress(NewLaunchpadAddress);
        ICustom(Params._Waitlist).____LaunchpadOverrideAddress(NewLaunchpadAddress);
    }

    /**
     * @dev Instantiates A New State

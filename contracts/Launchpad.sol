/**
 * @dev Interface For The Marketplace Contracts
 */
interface IMarketplace 
{ 
    struct DutchSale
    {
        string _Name;                     // [0] -> _Name
        uint _ProjectIDArtBlocks;         // [1] -> _ProjectIDArtBlocks
        uint _PriceStart;                 // [2] -> _PriceStart
        uint _PriceEnd;                   // [3] -> _PriceEnd
        uint _MaxAmtPerPurchase;          // [4] -> _MaxAmtPerPurchase
        uint _MaximumAvailableForSale;    // [5] -> _MaximumAvailableForSale
        uint _StartingBlockUnixTimestamp; // [6] -> _StartingBlockUnixTimestamp
        uint _SecondsBetweenPriceDecay;   // [7] -> _SecondsBetweenPriceDecay
        uint _SaleStrip;                  // [8] -> _SaleStrip note: For Traditional MintPack transferFrom() Sales 
    }

    struct DutchSaleInternal
    {
        address _NFT;           // [0] -> _NFT
        address _Operator;      // [1] _Operator (Wallet That NFT Is Pulling From)
        uint _CurrentIndex;     // [2] _CurrentIndex (If Simple Sale Type, This Is The Next Token Index To Iterate Upon)
        uint _Type;             // [3] _SaleType (0 = Simple, 1 = TransferFrom, 2 = PurchaseTo, 3 = MintPack)
        bool _ActivePublic;     // [4] -> _ActivePublic
        bool _ActiveBrightList; // [5] -> _ActiveBrightList 
        bool _Discount;         // [6] -> _Discount
        bool _ActiveRespend;    // [7] -> _ActiveRespend
    }

    struct SalePresale
    {
        string _Name;              // [0] -> _Name
        address _Operator;         // [1] -> _Operator
        address _NFT;              // [2] -> _NFT
        uint _MaxForSale;          // [3] -> _MaxForSale
        uint _MaxPerPurchase;      // [4] -> _MaxPerPurchase
        uint _PricePresale;        // [5] -> _PricePresale
        uint _PricePublic;         // [6] -> _PricePublic
        uint _TimestampEndFullSet; // [7] -> _TimestampEndFullSet
        uint _TimestampEndCitizen; // [8] -> _TimestampEndCitizen
        uint _TimestampSaleStart;  // [9] -> _TimestampSaleStart
        uint _Type;                // [10] -> _Type
        uint _ProjectID;           // [11] -> _ProjectID
    }

    struct SalePresaleRoots
    {
        bytes32 _RootEligibilityFullSet; // [0] -> _RootEligibilityFullSet
        bytes32 _RootEligibilityCitizen; // [1] -> _RootEligibilityCitizen
        bytes32 _RootAmountFullSet;      // [2] -> _RootAmountFullSet
        bytes32 _RootAmountCitizen;      // [3] -> _RootAmountCitizen
    }

    struct SaleFixedPrice
    {
        string _Name;                 // [0] -> _Name
        uint _Price;                  // [1] -> _Price
        uint _MintPassProjectID;      // [2] -> _MintPassProjectID
        uint _Type;                   // [3] -> _Type
        uint _MinterProjectID;        // [4] -> _MinterProjectID
        uint _AmountForSale;          // [5] -> _AmountForSale
        uint _TimestampStart;         // [6] -> _TimestampStart
        uint _CurrentIndex;           // [7] -> _CurrentIndex
        uint _BatchPurchaseAmount;    // [8] -> _BatchPurchaseAmount
        address _NFT;                 // [9] -> _NFT
        address _Operator;            // [10] -> _Operator
        bytes32[] _RootEligibilities; // [11] -> _RootEligibilities
        bytes32[] _RootAmounts;       // [12] -> _RootAmounts
    }

    /**
     * @dev Starts A Sale On The BasicMarketplace Contract
     * @param _Sale The Sale Parameters
     * @param _Roots The Sale Roots
     * @return MarketplaceSaleID
     */
    function _StartPresaleSale(SalePresale memory _Sale, SalePresaleRoots memory _Roots) external returns (uint);

    /**
     * @dev Starts A Fixed Price Sale On The BasicMarketplace Contract
     * @param _Sale The Sale Parameters
     * @param _Discounts The Sale Discounts
     * @return MarketplaceSaleID
     */
    function _StartFixedPriceSale(SaleFixedPrice memory _Sale, uint[] calldata _Discounts) external returns (uint);

    /**
     * @dev Initiates A New Sale On The DutchMarketplace Contract
     * @param _Sale The Sale Parameters
     * @param _State The Sale Internal State
     * @param RootsPrioriy The Merkle Roots For Priority
     * @param RootsAmounts The Merkle Roots For Amounts
     * @param DiscountAmounts The Discount Amounts
     * @return MarketplaceSaleID
     */
    function _StartSale (
        DutchSale memory _Sale, 
        DutchSaleInternal memory _State, 
        bytes32[] calldata RootsPrioriy, 
        bytes32[] calldata RootsAmounts, 
        uint[] calldata DiscountAmounts
    ) external returns ( uint );

    /**
     * @dev Clears A Dutch Auction & Initiates Refunds & Proceeds
     * @param SaleIndex The Index Of The Sale To Initiate Refunds & Proceeds For
     */
    function _InitiateRefundsAndProceeds (uint SaleIndex) external;

    /**
     * @dev Withdraws All ETH From The Marketplace To The Multisig
     */
    function _WithdrawETHFromSaleIndex(uint SaleIndex) external;
}

/**
 * @dev Interface For English Auction Contract
 */
interface IAuction
{
    struct Params
    {
        string _Name;                // [0] -> Name Of Auction
        bool _ClearingEnabled;       // [1] -> If Rebate Last Price Is Enabled (Everyone Pays Lowest Leaderboard Price)
        bool _UserSettlementEnabled; // [2] -> If Self-Service User Settlement Is Enabled (Bidders Can Settle Their Own Bids ETH & NFTs)
        uint _LeaderboardSize;       // [3] -> The Bid Threshold For NewMinimumBid (eg. 50 Valid Bids)
        uint _UnixStartTime;         // [4] -> Unix Start Time Of Auction
        uint _UnixEndTime;           // [5] -> Unix End Time Of Auction
        uint _MinBIPSIncrease;       // [6] -> Minimum BIPS (%) Increase On Each Subsequent Bid After Configured LeaderboardSize 
        uint _SecondsExtension;      // [7] -> # Of Second(s) Of Extension For Auction (Input In Seconds)
        uint _SecondsThreshold;      // [8] -> # Of Seconds Within Auction End Time To Be Eligible For Auction Extension
        uint _InitialMinimumBid;     // [9] -> Initial Minimum Bid
        uint _ProjectIDMintPass;     // [10] -> The Factory MintPass ProjectID
        uint _MintPackAmount;        // [11] -> The Amount Of Mint Passes To Be Dispersed Per Leaderboard Spot
        address _NFT;                // [12] -> Address Of NFT Contract
        address _Operator;           // [13] -> Wallet Holding NFTs To Disperse
    }

    /**
     * @dev Starts Auction
     * @param AuctionParams The Auction Parameters
     * @param MerkleRootsEligibilities The Merkle Roots For Eligibility
     * @param MerkleRootsAmounts The Merkle Roots For Amounts
     * @param DiscountAmounts The Discount Amounts
     * @return AuctionID
     */
    function _StartAuction (
        Params memory AuctionParams, 
        bytes32[] calldata MerkleRootsEligibilities, 
        bytes32[] calldata MerkleRootsAmounts, 
        uint[] calldata DiscountAmounts
    ) external returns (uint);

    /**
     * @dev Processes Auction Proceeds & NFTs (When TokenID Is Ambiguous)
     * @param AuctionID The AuctionID To Process
     */
    function _ProcessETHAndNFTsTokenIDsAmbiguous(uint AuctionID) external;

    /**
     * @dev Processes Auction Proceeds & NFTs (When TokenID Matters)
     * @param AuctionID The AuctionID To Process
     */
    function _ProcessETHAndNFTsTokenIDsDistinct(uint AuctionID) external;

    /**
     * @dev Processes Auction Proceeds & NFTs (When TokenID Is Unique) (Uses Leaderboard Ranking)
     * @param AuctionID The AuctionID To Process
     */
    function _ProcessETHAndNFTsTokenIDsSpecificRanked(uint AuctionID, uint[] calldata TokenIDs) external;

    /**
     * @dev Processes Auction Proceeds & NFTs Mint Pack Style (Uses Leaderboard Ranking)
     * @param AuctionID The AuctionID To Process
     */
    function _ProcessETHAndNFTsMintPacksRanked(uint AuctionID) external;
}

